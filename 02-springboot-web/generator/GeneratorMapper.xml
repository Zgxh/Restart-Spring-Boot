<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
		PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
		"http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>

	<!-- 指明数据库驱动包位置 -->
	<classPathEntry location="D:/apache-maven-3.5.4/repository/mysql/mysql-connector-java/8.0.19/mysql-connector-java-8.0.19.jar" />

	<!--配置table表的内容体，targetRuntime指定采用mybatis3版本-->
	<context id="DB2Tables" targetRuntime="MyBatis3">

		<!--压制注释的生成-->
		<commentGenerator>
			<property name="suppressAllComments" value="true" />
		</commentGenerator>

		<!-- 配置数据库连接信息 -->
		 <jdbcConnection driverClass="com.mysql.jdbc.Driver"
						 connectionURL="jdbc:mysql://localhost:3306/sql-learn"
						 userId="root"
						 password="zgxhsql">
		</jdbcConnection>

		<javaTypeResolver>
			<property name="forceBigDecimals" value="false" />
		</javaTypeResolver>

		<!-- 生成model类-->
		<javaModelGenerator targetPackage="com.zgxh.springboot.model" targetProject="src/main/java">
			<property name="enableSubPackages" value="true" />
			<property name="trimStrings" value="true" />
		</javaModelGenerator>

		<!-- 生成mybatis的Mapper.xml文件-->
		<sqlMapGenerator targetPackage="com.zgxh.springboot.mapper" targetProject="src/main/java">
			<property name="enableSubPackages" value="true" />
		</sqlMapGenerator>

		<!-- 生成mybatis的Mapper接口类文件-->
		<javaClientGenerator type="XMLMAPPER" targetPackage="com.zgxh.springboot.mapper" targetProject="src/main/java">
			<property name="enableSubPackages" value="true" />
		</javaClientGenerator>

		<!-- 要生成那些表(更改tableName和domainObjectName就可以) -->
		<table tableName="303member"
			   domainObjectName="MemberInfo"
			   enableCountByExample="false"
			   enableUpdateByExample="false"
			   enableDeleteByExample="false"
			   enableSelectByExample="false"
			   selectByExampleQueryId="false" />
<!--		<table tableName="course_info" domainObjectName="CourseInfo" enableCountByExample="false" enableUpdateByExample="false" enableDeleteByExample="false" enableSelectByExample="false" selectByExampleQueryId="false" />-->
<!--		<table tableName="course_user_info" domainObjectName="CourseUserInfo" enableCountByExample="false" enableUpdateByExample="false" enableDeleteByExample="false" enableSelectByExample="false" selectByExampleQueryId="false" />-->
	</context>

</generatorConfiguration>